
  <div class="row">
    <div class="col-md-12">
      <div class="home-jumbotron">
        <div id="hp-jt-carousel" class="carousel slide" data-ride="carousel">
          <!-- Indicators -->
          <ol class="carousel-indicators">
            <li data-target="#hp-jt-carousel" data-slide-to="0" class="active"></li>
            <li data-target="#hp-jt-carousel" data-slide-to="1"></li>
            <li data-target="#hp-jt-carousel" data-slide-to="2"></li>
          </ol>
          <!-- Wrapper for slides -->
          <div class="carousel-inner">
            <div class="item active">
              <img id="deadpool" src="/assets/Deadpool-and-Vanessa.jpg" alt="Deadpool and Vanessa">
            </div>
            <div class="item">
              <img id="superman" src="/assets/superman.jpg" alt="Superman and Lois">
            </div>
            <div class="item">
              <img id="shamy" src="/assets/shamy-kiss.jpg" alt="Sheldon and Amy kiss">
            </div>
          </div>
        </div>
      </div>
      <div class="home-jumbotron-text">
        <% unless customer_signed_in? %>
          <h1>JOIN TODAY!</h1>
          <%= link_to new_customer_registration_path do %>
            <p>REGISTER</p>
          <% end %>
        <% else %>
          <h1>Welcome, <%= current_customer.username.capitalize %>!<h1>
        <% end %>
      </div>
    </div>
  </div>



